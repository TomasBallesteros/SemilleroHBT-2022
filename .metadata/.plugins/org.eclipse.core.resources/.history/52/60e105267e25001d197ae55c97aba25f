 */
public enum estadoEnum {
	ACTIVO("Activo"),
	INACTIVO("Inactivo"),
	
	;
	
	private String estado;
	
	estadoEnum(String estado){
		this.estado = estado;
	}
	
	public String getEstado() {
		return estado;
	}

}
